
def isspace ( char -- bool )
	dup 0x20 eq if 1 else ( Space )
	dup 0x0a eq if 1 else ( Newline )
	dup 0x0d eq if 1 else ( Carriage Return )
	dup 0x09 eq if 1 else ( Tab )

	0 ( None of the above )

	end end end end

	swap drop ( Remove the character )
end

def isdigit ( char -- bool )
	dup 48 geq if ( Greater/equal to '0' )
		57 leq if ( Less/equal to '9' )
			1
		else 0 end
	else drop 0 end
end
